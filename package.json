{
  "name": "world-radio-league-challenge",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "build:dev": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "build:watch": "webpack --watch --mode development",
    "dev": "concurrently \"npm run build:watch\" \"nodemon --ignore src ./bin/www\"",
    "format": "prettier --write \"**/*.{js,jsx}\" --ignore-path .gitignore --ignore-unknown \"!public/**\"",
    "lint": "eslint . --ext .js,.jsx --ignore-pattern public/",
    "lint:fix": "eslint . --ext .js,.jsx --fix --ignore-pattern public/",
    "migrate": "sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all",
    "start": "node ./bin/www",
    "undo:migrate": "sequelize-cli db:migrate:undo:all",
    "undo:seed": "sequelize-cli db:seed:undo:all"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-validator": "^7.2.1",
    "firebase-admin": "^13.5.0",
    "http-errors": "~1.6.3",
    "http-status": "^2.1.0",
    "morgan": "~1.9.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/eslint-parser": "^7.28.4",
    "@babel/node": "^7.28.0",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@tanstack/react-table": "^8.21.3",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "babel-loader": "^10.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "typescript": "^5.9.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1"
  }
}
